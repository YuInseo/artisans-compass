  "main": "dist-electron/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/YuInseo/artisans-compass"
  },
  "build": {
    "appId": "com.artisans.compass",
    "productName": "Artisans Compass",
    "asar": true,
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "YuInseo",
        "repo": "artisans-compass"
      }
    ],
    "win": {
      "target": [
        "nsis"
